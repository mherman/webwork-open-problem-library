);
STOP
generate_perl_variables_from_constants($text,"a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p");
$func1= Formula(" $a*x^3+$b*x^2 + $c*x + $d ")->reduce;
$func2= Formula("$e*x^3 + $f*x^2 + $g*x + $h")->reduce;
$func3 = Formula("$i*x^3 + $j*x^2 + $k*x + $l")->reduce;
$func4 = Formula("$m*x^3 + $n*x^2 + $o*x + $p")->reduce;
$func5 = Formula("$q*x^3 + $r*x^2 + $s*x + $t")->reduce;
Context()->texStrings;
BEGIN_TEXT
Suppose \(T \colon P_3({\mathbb R}) \to P_3({\mathbb R}) \) is a linear map such that $PAR
$BCENTER
\(T(x^3) = $func1\), \(T(x^2) = $func2 \), \(T(x) = $func3 \), and \(T(1) = $func4\).
$ECENTER
$PAR
Compute \(T($func5)\) \{InTermsOfString()\}.$PAR
\(T($func5) = \)\{ans_rule(60)\}

END_TEXT

$answer = Compute("($a*$q + $e*$r + $i*$s + $m*$t)*x^3 + ($b*$q + $f*$r+$j*$s+$n*$t)*x^2 + ($c*$q+$g*$r + $k*$s+$o*$t)*x + $d*$q+$h*$r+$l*$s+$p*$t");
ANS($answer->cmp);


